{
  "expo": {
    "name": "Todoリスト",
    "slug": "react-native-tutorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "reactnativetutorial",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.lainy.react.native.tutorial",
      "supportsTablet": false
    },
    "android": {
      "versionCode": 1,
      "package": "com.lainy.react.native.tutorial",
      "icon": "./assets/android_icon.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "jsEngine": "jsc"
  }
}
